{
  "name": "aseen",
  "version": "0.1.0",
  "author": "Landon Fuhr",
  "description": "Automated behavioural analysis.",
  "main": "dist/electron.js",
  "scripts": {
    "[[[ --- MAKE --- ]]]": "",
    "make:run": "electron-forge make",
    "make": "run-s   build   make:run",
    "[[[ --- BUILD --- ]]]": "",
    "clean": "node builder/clean.js",
    "build:run": "run-p   *:build",
    "build": "run-s   clean   build:run",
    "[[[ --- ELECTRON --- ]]]": "",
    "electron:dev": "(npm run electron:build) && (wait-on tcp:3000) && (electron . --debug)",
    "electron:watch": "nodemon --exec npm run electron:dev",
    "electron": "cross-env ELECTRON_IS_DEV=0 electron .",
    "electron:build": "run-s   electron:build:main   electron:build:splash",
    "electron:build:main": "webpack --config-name main",
    "electron:build:splash": "node builder/splash.js",
    "[[[ --- REACT --- ]]]": "",
    "react:watch": "(cd src/frontend) && (npm run start)",
    "react:watch-mock-electron": "cross-env REACT_APP_MOCK_LEVEL=WEB npm run react:watch",
    "react:watch-mock-analyzer": "cross-env REACT_APP_MOCK_LEVEL=ELECTRON npm run react:watch",
    "react:build": "(cd src/frontend) && (cross-env BUILD_PATH=../../dist/build-frontend npm run build) && (cd../..)",
    "[[[ --- VIDEO CREATOR --- ]]]": "",
    "video-creator:clean": "node builder/video_creator/clean.js",
    "video-creator:build:py": "node builder/video_creator/py.js",
    "video-creator:build:node": "webpack --config-name video_creator",
    "video-creator:build": "run-s   video-creator:clean   video-creator:build:py   video-creator:build:node",
    "[[[ --- BEHAVIOUR --- ]]]": "",
    "behaviour:clean": "node builder/behaviour/clean.js",
    "behaviour:build:py": "node builder/behaviour/py.js",
    "behaviour:build:node": "webpack --config-name behaviour",
    "behaviour:build": "run-s   behaviour:clean   behaviour:build:py   behaviour:build:node",
    "[[[ --- TRACKING --- ]]]": "",
    "tracking:clean": "node builder/tracking/clean.js",
    "tracking:build:py": "node builder/tracking/py.js",
    "tracking:build:node": "webpack --config-name tracking",
    "tracking:build": "run-s   tracking:clean   tracking:build:py   tracking:build:node",
    "[[[ --- COMBINED --- ]]]": "",
    "combined:watch:react-electron": "run-p -r   react:watch   electron:watch",
    "[[[ --- TEST --- ]]]": "",
    "test:unit:analysis-modules": "jest .unit.test.ts --config src/analysis_modules/jest.config.js",
    "test:unit:electron": "jest .unit.test.ts --config src/main/jest.config.js",
    "test:unit:py": "pytest -m \"not integration\"",
    "test:unit": "run-p test:unit:*",
    "test:integration:electron": "jest .integration.test.ts --config src/main/jest.config.js",
    "test:integration:py": "pytest -m \"integration\"",
    "test:integration": "run-p test:integration:*",
    "test:react": "cross-env CI=true npm t --prefix src/frontend",
    "test": "run-s test:*"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "ffprobe": "^1.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@types/ffprobe": "^1.1.1",
    "@types/ffprobe-static": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.2",
    "cross-env": "^7.0.3",
    "electron": "^13.0.1",
    "electron-icon-builder": "^2.0.1",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "spectron": "^15.0.0",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
