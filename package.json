{
  "name": "aseen",
  "version": "0.1.0",
  "author": "Landon Fuhr",
  "description": "Automated behavioural analysis.",
  "main": "dist/electron.js",
  "scripts": {
    "[[[ --- BUILD --- ]]]": "",
    "build:prod:clean": "ts-node builder/clean.ts",
    "build:prod:run": "run-p   *:build:prod",
    "build:prod": "run-s   build:prod:clean   build:prod:run",
    "make:run": "electron-forge make",
    "make": "run-s   build:prod   make:run",
    "package:run": "electron-forge package",
    "package": "run-s   build:prod   package:run",
    "[[[ --- ELECTRON --- ]]]": "",
    "electron:dev": "(wait-on tcp:3000) && (nodemon --exec \"electron . --debug --no-sandbox\" --watch dist/electron.js)",
    "electron:watch": "run-p   electron:build:dev   electron:dev",
    "electron": "cross-env ELECTRON_IS_DEV=0 electron .",
    "electron:build:dev": "run-p   electron:build:main:dev   electron:build:extras",
    "electron:build:prod": "run-p   electron:build:main:prod   electron:build:extras",
    "electron:build:main:dev": "npm run electron:build:main:base -- dev --watch",
    "electron:build:main:prod": "npm run electron:build:main:base -- prod",
    "electron:build:main:base": "webpack --config ./src/main/webpack.config.js --config-name",
    "electron:build:extras": "run-p   electron:build:ffprobe   electron:build:splash",
    "electron:build:ffprobe": "ts-node --transpile-only builder/ffprobe.ts",
    "electron:build:splash": "ts-node --transpile-only builder/splash.ts",
    "[[[ --- REACT --- ]]]": "",
    "react:watch": "npm run start --prefix ./src/frontend",
    "react:watch-mock-electron": "cross-env REACT_APP_MOCK_LEVEL=WEB npm run react:watch",
    "react:watch-mock-analyzer": "cross-env REACT_APP_MOCK_LEVEL=ELECTRON npm run react:watch",
    "react:build:prod": "(cd src/frontend) && (cross-env BUILD_PATH=../../dist/build-frontend npm run build) && (cd../..)",
    "[[[ --- VIDEO CREATOR --- ]]]": "",
    "video-creator:clean": "ts-node builder/video_creator/clean.ts",
    "video-creator:build:py": "ts-node builder/video_creator/py.ts",
    "video-creator:build:node:dev": "npm run analysis-modules:build:node:base -- video_creator_dev",
    "video-creator:build:node:prod": "npm run analysis-modules:build:node:base -- video_creator_prod",
    "video-creator:build:dev": "run-s   video-creator:clean   video-creator:build:py   video-creator:build:node:dev",
    "video-creator:build:prod": "run-s   video-creator:clean   video-creator:build:py   video-creator:build:node:prod",
    "[[[ --- BEHAVIOUR --- ]]]": "",
    "behaviour:clean": "ts-node builder/behaviour/clean.ts",
    "behaviour:build:py": "ts-node builder/behaviour/py.ts",
    "behaviour:build:node:dev": "npm run analysis-modules:build:node:base -- behaviour_dev",
    "behaviour:build:node:prod": "npm run analysis-modules:build:node:base -- behaviour_prod",
    "behaviour:build:dev": "run-s   behaviour:clean   behaviour:build:py   behaviour:build:node:dev",
    "behaviour:build:prod": "run-s   behaviour:clean   behaviour:build:py   behaviour:build:node:prod",
    "[[[ --- TRACKING --- ]]]": "",
    "tracking:clean": "ts-node builder/tracking/clean.ts",
    "tracking:build:py": "ts-node builder/tracking/py.ts",
    "tracking:build:node:dev": "npm run analysis-modules:build:node:base -- tracking_dev",
    "tracking:build:node:prod": "npm run analysis-modules:build:node:base -- tracking_prod",
    "tracking:build:dev": "run-s   tracking:clean   tracking:build:py   tracking:build:node:dev",
    "tracking:build:prod": "run-s   tracking:clean   tracking:build:py   tracking:build:node:prod",
    "[[[ --- ANALYSIS MODULES --- ]]]": "",
    "analysis-modules:build:node:base": "webpack --config ./src/analysis_modules/webpack.config.js --config-name",
    "[[[ --- COMBINED --- ]]]": "",
    "combined:watch:react-electron": "run-p -r   react:watch   electron:watch",
    "[[[ --- TEST --- ]]]": "",
    "test:unit:analysis-modules": "jest .unit.test.ts --config src/analysis_modules/jest.config.js",
    "test:unit:electron": "jest .unit.test.ts --config src/main/jest.config.js",
    "test:unit:py": "pytest -m \"not integration\"",
    "test:unit": "run-p test:unit:*",
    "test:integration:electron": "jest .integration.test.ts --config src/main/jest.config.js",
    "test:integration:py": "pytest -m \"integration\"",
    "test:integration": "run-p test:integration:*",
    "test:react": "cross-env CI=true npm t --prefix src/frontend",
    "test": "run-s test:*",
    "[[[ --- DOCUMENTATION --- ]]]": "",
    "docs:watch": "retype watch"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.0",
    "ffprobe": "^1.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@types/ffprobe": "^1.1.1",
    "@types/ffprobe-static": "^2.0.0",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.2",
    "cross-env": "^7.0.3",
    "electron": "^13.0.1",
    "electron-icon-builder": "^2.0.1",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "retypeapp": "^1.11.1",
    "spectron": "^15.0.0",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "yaml": "^1.10.2"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
